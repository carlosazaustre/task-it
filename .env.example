# Database
# Port 5433 is used to avoid conflicts with local PostgreSQL installations (docker-compose maps 5433 -> 5432)
DATABASE_URL="postgresql://taskit:taskit@localhost:5433/taskit?schema=public"

# Auth (generate secret with: npx auth secret)
AUTH_SECRET="generate-with-npx-auth-secret"
AUTH_URL="http://localhost:3000"

# App
NEXT_PUBLIC_APP_URL="http://localhost:3000"
NODE_ENV="development"
